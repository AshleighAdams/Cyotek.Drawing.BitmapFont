<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Title>Cyotek BitmapFont</Title>
    <Authors>Cyotek</Authors>
    <Owners>Cyotek</Owners>
    <VersionPrefix>2.0</VersionPrefix>
    <VersionSuffix>beta1</VersionSuffix>
    <Copyright>Copyright Â© 2012-2020 Cyotek Ltd. All Rights Reserved.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/cyotek/Cyotek.Data.Nbt</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/cyotek/Cyotek.Drawing.BitmapFont</RepositoryUrl>
    <Description>This library is a generic parser for the AngelCode BMFont format, supporting the text and XML versions of the format.</Description>
    <Summary>Generic parser for the AngelCode BMFont format, supporting the text and XML versions of the format.</Summary>
    <PackageTags>angelcode;bitmapfont;bmfont;bitmap;font;cyotek</PackageTags>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>
